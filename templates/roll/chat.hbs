<div class="trued6">
  <div class="dice-roll">
    {{#if flavor}}
    <div class="dice-flavor">{{flavor}}</div>
    {{/if}}
    <div class="dice-result">
      {{{tooltip}}}
      <h4 class="dice-total {{cssClass}}">
        {{localize rollResult}}
        {{#if damage}}
        <br>
        <label>{{localize "TRUED6.DiceRoll.Damage"}}: {{damage}}</label>
        {{/if}}
      </h4>
    </div>
  </div>
</div>